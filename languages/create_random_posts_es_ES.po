msgid "Number of posts to create (required):"
msgstr "Número de entradas a crear (obligatorio):"

#: admin/configuration.php:15
msgid "Title prefix (required):"
msgstr "Prefijo del título (obligatorio):"

#: admin/configuration.php:18
msgid "Featured image:"
msgstr "Imagen destacada:"

#: admin/configuration.php:21
msgid "Category of the post:"
msgstr "Categoría de la entrada:"

#: admin/configuration.php:23
msgid "Text to be included in the posts (required):"
msgstr "Texto de las entradas (obligatorio):"

#: admin/configuration.php:26
msgid "Create posts"
msgstr "Crear entradas"

